{
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^6.0.4",
    "browser-sync": "^3.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.6.2",
    "prettier-plugin-classnames": "^0.8.4",
    "prettier-plugin-merge": "^0.8.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "rimraf": "^6.0.1",
    "serve": "^14.2.5"
  },
  "scripts": {
    "dev": "run-s build:initial && run-p css:watch eleventy:watch server",
    "build": "run-s clean css:build eleventy:build",
    "clean": "rimraf _site",
    "css:watch": "tailwindcss -i src/css/input.css -o src/css/style.css --watch --minify",
    "css:build": "tailwindcss -i src/css/input.css -o src/css/style.css --minify",
    "eleventy:watch": "eleventy --watch --quiet",
    "eleventy:build": "eleventy",
    "server": "browser-sync start --server _site --files _site --port 8080 --no-open --no-notify --ghost-mode false",
    "build:initial": "run-s clean css:build eleventy:build:quiet",
    "eleventy:build:quiet": "eleventy --quiet",
    "format": "prettier --write .",
    "start": "pnpm run dev",
    "preview": "run-s build && browser-sync start --server _site --port 8080 --no-open --no-notify --ghost-mode false",
    "serve:dist": "serve _site -p 8080 -s --host 0.0.0.0",
    "dev:open": "start chrome http://localhost:8080",
    "gemini-commit": "bash autocommit.sh",
    "push": "git push origin main"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.14",
    "tailwindcss": "^4.1.14"
  },
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac"
}
